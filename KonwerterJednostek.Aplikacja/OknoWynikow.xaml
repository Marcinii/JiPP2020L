    </Window.Resources>
    <Window.Triggers>
    </Window.Triggers>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.3*"></RowDefinition>
            <RowDefinition Height="0.6*"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0">
            <Grid Background="Black">
                <Label FontSize="28" Foreground="White">WYNIKI</Label>
            </Grid>
            <StackPanel>
                <Grid Height="40">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.4*"></ColumnDefinition>
                        <ColumnDefinition Width="0.1*"></ColumnDefinition>
                        <ColumnDefinition Width="0.4*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Button x:Name="PoprzednniaStronaPrzycisk" Foreground="White" Background="Black" Grid.Column="0" Margin="120,0,0,0">Poprzednia</Button>
                    <Label x:Name="StronaPole" Content="1" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" ></Label>
                    <Button x:Name="NastepnaStronaPrzycisk" Foreground="White" Background="Black" Grid.Column="2" Margin="0,0,120,0" >Nastepna</Button>
                </Grid>
                <Label>KONWERTER:</Label>
                <ComboBox x:Name="KonwerterWybor" DisplayMemberPath="Nazwa"></ComboBox>
                <Label>OD:</Label>
                <DatePicker x:Name="DataPoczatekWybor"></DatePicker>
                <Label>DO:</Label>
                <DatePicker x:Name="DataKoniecWybor"></DatePicker>
                <Button x:Name="WczytajDanePrzycisk" Foreground="White" Background="Black" Margin="120,45,120,0">WCZYTAJ</Button>
            </StackPanel>
        </StackPanel>
        <StackPanel Grid.Row="1">
            <ListView x:Name="Wyniki">
                <ListView.View>
                    <GridView>
                        <GridViewColumn Header="id" DisplayMemberBinding="{Binding id}"></GridViewColumn>
                        <GridViewColumn Header="konwerter" DisplayMemberBinding="{Binding konwerter}"></GridViewColumn>
                        <GridViewColumn Header="data" DisplayMemberBinding="{Binding data}"></GridViewColumn>
                        <GridViewColumn Header="wejscie" DisplayMemberBinding="{Binding wartoscWejsciowa}"></GridViewColumn>
                        <GridViewColumn Header="jednostka" DisplayMemberBinding="{Binding jednostkaWejsciowa}"></GridViewColumn>
                        <GridViewColumn Header="wyjscie" DisplayMemberBinding="{Binding wartoscWyjsciowa}"></GridViewColumn>
                        <GridViewColumn Header="jednostka" DisplayMemberBinding="{Binding jednostkaWyjsciowa}"></GridViewColumn>
                    </GridView>
                </ListView.View>
            </ListView>
        </StackPanel>
        <Rectangle x:Name="rectangle" Fill="#BF000000" Visibility="Hidden" HorizontalAlignment="Left" Height="969" Grid.RowSpan="2" Stroke="Black" VerticalAlignment="Top" Width="480.39"/>
        <Ellipse x:Name="ellipse" Fill="#BF000000" HorizontalAlignment="Left" Visibility="Hidden" Height="109.5" Margin="173,89.5,0,0" Grid.Row="1" Stroke="Black" VerticalAlignment="Top" Width="117" RenderTransformOrigin="0.5,0.5">
            <Ellipse.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Ellipse.RenderTransform>
        </Ellipse>
        <Ellipse x:Name="ellipse1" Fill="#BFFF0000" HorizontalAlignment="Left" Visibility="Hidden" Height="15.333" Margin="221.667,96,0,0" Grid.Row="1" Stroke="Black" VerticalAlignment="Top" Width="16" RenderTransformOrigin="0.5,0.5">
            <Ellipse.RenderTransform>
