        <Ellipse x:Name="ellipse3" Fill="#BFFF0000" HorizontalAlignment="Left" Visibility="Hidden" Height="15.333" Margin="178.667,137,0,0" Grid.Row="1" Stroke="Black" VerticalAlignment="Top" Width="16" RenderTransformOrigin="0.5,0.5">
            <Ellipse.RenderTransform>
