<Window x:Class="Todo.Desktop.TodoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Todo.Desktop"
        xmlns:custom="clr-namespace:Todo.CustomControls;assembly=Todo.CustomControls"
        mc:Ignorable="d"
        Title="TodoWindow" Height="450" Width="800">
    <Grid>
        <custom:TodoView x:Name="TodoView" Width="auto" Height="auto" TodoCompletedChanged="TodoView_TodoCompletedChanged"></custom:TodoView>
    </Grid>
</Window>
