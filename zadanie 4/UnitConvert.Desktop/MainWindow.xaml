<Window x:Class="UnitConvert.Desktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnitConvert.Desktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="inputTextBox" HorizontalAlignment="Left" Height="23" Margin="162,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Wartość do konwersji" HorizontalAlignment="Left" Margin="34,72,0,0" VerticalAlignment="Top"/>
        <Button Content="Naciśnij mnie" HorizontalAlignment="Left" Margin="303,75,0,0" VerticalAlignment="Top" Width="107" Height="23" Click="Button_Click" Background="#FFFF5F5F"/>
        <TextBlock x:Name="outputTextBlock" HorizontalAlignment="Left" Margin="434,72,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="26" Width="79"/>
        <ComboBox x:Name="combobox" HorizontalAlignment="Left" Margin="148,173,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="combobox_SelectionChanged"/>
    </Grid>
</Window>
